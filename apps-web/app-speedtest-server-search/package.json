{
  "name": "@monorepo/app-speedtest-server-search",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "imports": {
    "#src/*": "./src/*"
  },
  "scripts": {
    "build": "next build",
    "build:deps": "pnpm --filter '{.}^...' --workspace-concurrency=-1 run build",
    "dev": "next dev --turbopack",
    "lint:eslint": "eslint . --cache --cache-strategy content --cache-location ./node_modules/.cache/eslint/eslintcache.json --max-warnings 0 --report-unused-disable-directives",
    "preview": "next start",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:update": "vitest run --update",
    "test:watch": "vitest --watch",
    "tsc:build": "tsgo --build tsconfig.json",
    "tsc:build:clean": "tsgo --build tsconfig.json --clean",
    "tsc:build:upstream": "pnpm --filter '{.}...' --workspace-concurrency=-2 run tsc:build",
    "tsc:build:watch": "tsgo --build tsconfig.json --watch"
  },
  "dependencies": {
    "@monorepo/tailwindcss": "workspace:^",
    "@monorepo/utils": "workspace:^",
    "@tanstack/react-query": "catalog:",
    "data-fns": "catalog:",
    "es-toolkit": "catalog:",
    "next": "catalog:",
    "ofetch": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@monorepo/tsconfig": "workspace:^",
    "@tailwindcss/postcss": "catalog:",
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@typescript/native-preview": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  }
}
